\n\t\t\t\t\tname: pet.name,\n\t\t\t\t\tanimal: pet.animal,\n\t\t\t\t\tlocation: `${pet.contact.city}, ${pet.contact.state}`,\n\t\t\t\t\tmedia: pet.media,\n\t\t\t\t\tbreed,\n\t\t\t\t\tdescription: pet.description,\n\t\t\t\t\tloading: false\n\t\t\t\t});\n\t\t\t})\n\t\t\t.catch(err => {\n\t\t\t\tthis.setState({ error: err });\n\t\t\t});\n\t}\n\n\trender() {\n\t\tif (this.state.loading) {\n\t\t\treturn <h1>Loading ...</h1>;\n\t\t}\n\t\tconst { name, animal, breed, location, description } = this.state;\n\t\treturn (\n\t\t\t<div className=\"details\">\n\t\t\t\t<div>\n\t\t\t\t\t<h1>{name}</h1>\n\t\t\t\t\t<h2>\n\t\t\t\t\t\t{animal} - {breed} - {location}\n\t\t\t\t\t</h2>\n\t\t\t\t\t<p>{description}</p>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t);\n\t}\n}\n\nexport default Details;\n"},"lineCount":null}},"error":null,"hash":"3447fbd9e847c0682fe464325c1413da","cacheData":{"env":{"API_KEY":"VlwRcD4uobfKhJ3vc8qJPe1RgZ8EFWe9uPxH9mJxbEOsjly3ea","API_SECRET":"E6UL8xwumxdQTqgJ6CsMfYjl8deQhMYcD7aiveqV"}}}